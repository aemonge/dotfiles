ProxyCommand /usr/bin/corkscrew http.proxy 8080 %h %p ~/.ssh/myauth

Host ssh.github.com
  HostName ssh.github.com
  User git
  Port 443
  IdentityFile ~/.ssh/id_aemGh_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes

